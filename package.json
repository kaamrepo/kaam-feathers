    "aws-sdk": "^2.1641.0",
    "cloudinary": "^1.37.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "firebase-admin": "^11.9.0",
    "mongodb": "^5.6.0",
    "multer": "^1.4.5-lts.1",
    "twilio": "^4.11.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.6",
